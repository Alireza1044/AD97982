/* You can visualize the graph with: 
https://dreampuf.github.io/GraphvizOnline/  Set Engine to fdp
https://graphviz.herokuapp.com/ 
http://www.webgraphviz.com/ 

Also if the graph layout doesn't look good try
changing the splines setting below to:
ortho, curved, line, polyline, spline */

digraph G {
   graph[splines = "curved"];
   node[height = 0.1, width = 0.1];
   "1" [pos="-2,0!"];
   "2" [pos="-2,3!"];
   "3" [pos="3,0!"];
   "4" [pos="0,0!"];
   "5" [pos="1,-3!"];
   "6" [pos="3,-2!"];
   "7" [pos="2,1!"];
   "6" -> "5" [label="3", color="#f032e6", fontcolor="#f032e6"];
   "1" -> "5" [label="6", color="#9a6324", fontcolor="#9a6324"];
   "3" -> "2" [label="7", color="#4363d8", fontcolor="#4363d8"];
   "5" -> "6" [label="3", color="#3cb44b", fontcolor="#3cb44b"];
   "2" -> "1" [label="5", color="#808000", fontcolor="#808000"];
   "5" -> "4" [label="5", color="#000000", fontcolor="#000000"];
   "2" -> "4" [label="4", color="#808080", fontcolor="#808080"];
   "1" -> "4" [label="3", color="#e6beff", fontcolor="#e6beff"];
   "2" -> "3" [label="6", color="#000075", fontcolor="#000075"];
   "4" -> "6" [label="4", color="#aaffc3", fontcolor="#aaffc3"];
   "3" -> "4" [label="4", color="#911eb4", fontcolor="#911eb4"];
   "7" -> "2" [label="5", color="#e6beff", fontcolor="#e6beff"];
   "4" -> "2" [label="4", color="#008080", fontcolor="#008080"];
   "3" -> "7" [label="2", color="#bcf60c", fontcolor="#bcf60c"];
   "4" -> "3" [label="5", color="#e6beff", fontcolor="#e6beff"];
   "5" -> "1" [label="5", color="#ffd8b1", fontcolor="#ffd8b1"];
   "5" -> "7" [label="5", color="#ffe119", fontcolor="#ffe119"];
   "3" -> "5" [label="5", color="#46f0f0", fontcolor="#46f0f0"];
   "1" -> "2" [label="5", color="#fabebe", fontcolor="#fabebe"];
   "3" -> "6" [label="3", color="#f032e6", fontcolor="#f032e6"];
   "5" -> "2" [label="7", color="#000075", fontcolor="#000075"];
   "5" -> "3" [label="5", color="#808080", fontcolor="#808080"];
}