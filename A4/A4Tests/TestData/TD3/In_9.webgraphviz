/* You can visualize the graph with: 
https://dreampuf.github.io/GraphvizOnline/  Set Engine to fdp
https://graphviz.herokuapp.com/ 
http://www.webgraphviz.com/ 

Also if the graph layout doesn't look good try
changing the splines setting below to:
ortho, curved, line, polyline, spline */

digraph G {
   graph[splines = "curved"];
   node[height = 0.1, width = 0.1];
   "1" [pos="-2,0!"];
   "2" [pos="-2,3!"];
   "3" [pos="3,0!"];
   "4" [pos="0,0!"];
   "5" [pos="1,-3!"];
   "6" [pos="3,-2!"];
   "5" -> "1" [label="6", color="#ffd8b1", fontcolor="#ffd8b1"];
   "2" -> "4" [label="4", color="#808080", fontcolor="#808080"];
   "6" -> "4" [label="4", color="#46f0f0", fontcolor="#46f0f0"];
   "6" -> "5" [label="4", color="#f032e6", fontcolor="#f032e6"];
   "4" -> "3" [label="5", color="#e6beff", fontcolor="#e6beff"];
   "4" -> "1" [label="3", color="#fabebe", fontcolor="#fabebe"];
   "1" -> "4" [label="4", color="#e6beff", fontcolor="#e6beff"];
   "3" -> "4" [label="4", color="#911eb4", fontcolor="#911eb4"];
   "4" -> "5" [label="4", color="#800000", fontcolor="#800000"];
   "5" -> "3" [label="4", color="#808080", fontcolor="#808080"];
   "6" -> "3" [label="3", color="#911eb4", fontcolor="#911eb4"];
   "2" -> "1" [label="5", color="#808000", fontcolor="#808000"];
   "1" -> "5" [label="6", color="#9a6324", fontcolor="#9a6324"];
   "4" -> "2" [label="4", color="#008080", fontcolor="#008080"];
   "5" -> "6" [label="3", color="#3cb44b", fontcolor="#3cb44b"];
   "1" -> "2" [label="4", color="#fabebe", fontcolor="#fabebe"];
}