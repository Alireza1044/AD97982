/* You can visualize the graph with: 
https://dreampuf.github.io/GraphvizOnline/  Set Engine to fdp
https://graphviz.herokuapp.com/ 
http://www.webgraphviz.com/ 

Also if the graph layout doesn't look good try
changing the splines setting below to:
ortho, curved, line, polyline, spline */

digraph G {
   graph[splines = "curved"];
   node[height = 0.1, width = 0.1];
   "1" [pos="1,3!"];
   "2" [pos="-2,-4!"];
   "3" [pos="1,1!"];
   "4" [pos="-1,4!"];
   "5" [pos="-4,1!"];
   "6" [pos="2,4!"];
   "7" [pos="2,0!"];
   "8" [pos="3,-2!"];
   "1" -> "6" [label="2", color="#800000", fontcolor="#800000"];
   "7" -> "3" [label="2", color="#9a6324", fontcolor="#9a6324"];
   "6" -> "3" [label="5", color="#911eb4", fontcolor="#911eb4"];
   "8" -> "3" [label="4", color="#000000", fontcolor="#000000"];
   "1" -> "4" [label="3", color="#e6beff", fontcolor="#e6beff"];
   "6" -> "1" [label="2", color="#4363d8", fontcolor="#4363d8"];
   "3" -> "7" [label="2", color="#bcf60c", fontcolor="#bcf60c"];
   "4" -> "3" [label="4", color="#e6beff", fontcolor="#e6beff"];
   "6" -> "8" [label="8", color="#008080", fontcolor="#008080"];
   "1" -> "3" [label="3", color="#008080", fontcolor="#008080"];
   "5" -> "1" [label="6", color="#ffd8b1", fontcolor="#ffd8b1"];
   "6" -> "5" [label="7", color="#f032e6", fontcolor="#f032e6"];
   "6" -> "4" [label="4", color="#46f0f0", fontcolor="#46f0f0"];
   "3" -> "1" [label="3", color="#ffe119", fontcolor="#ffe119"];
   "3" -> "6" [label="4", color="#f032e6", fontcolor="#f032e6"];
   "1" -> "7" [label="5", color="#aaffc3", fontcolor="#aaffc3"];
   "7" -> "1" [label="4", color="#008080", fontcolor="#008080"];
   "7" -> "4" [label="7", color="#800000", fontcolor="#800000"];
   "2" -> "8" [label="6", color="#ffe119", fontcolor="#ffe119"];
   "5" -> "7" [label="7", color="#ffe119", fontcolor="#ffe119"];
   "6" -> "7" [label="6", color="#fabebe", fontcolor="#fabebe"];
   "3" -> "4" [label="5", color="#911eb4", fontcolor="#911eb4"];
}